	removeReviewer = group user/Dan Fodorean (SideofClouds)
	submit = group user/Dan Fodorean (SideofClouds)
[access "refs/heads/*"]
	read = group user/Dan Fodorean (SideofClouds)
	create = group user/Dan Fodorean (SideofClouds)
