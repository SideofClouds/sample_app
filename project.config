	pushMerge = group user/Dan Fodorean (SideofClouds)
[access "refs/tags/*"]
	push = +force group user/Dan Fodorean (SideofClouds)
	pushTag = group user/Dan Fodorean (SideofClouds)
	pushSignedTag = group user/Dan Fodorean (SideofClouds)
